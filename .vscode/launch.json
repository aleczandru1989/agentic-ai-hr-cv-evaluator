{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File (simple)",
            "type": "debugpy",
            "request": "launch",
            "program": "app.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        }
    ]
}